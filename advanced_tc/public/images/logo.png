  <circle cx="32" cy="32" r="30" fill="#3498db" stroke="#2980b9" stroke-width="2"/>
  
  <!-- Calendar icon -->
  <rect x="16" y="20" width="32" height="28" rx="2" fill="white" stroke="#34495e" stroke-width="1.5"/>
  
  <!-- Calendar header -->
  <rect x="16" y="20" width="32" height="8" rx="2" fill="#34495e"/>
  
  <!-- Calendar rings -->
  <circle cx="22" cy="16" r="2" fill="#34495e"/>
  <circle cx="42" cy="16" r="2" fill="#34495e"/>
  
  <!-- Calendar grid lines -->
  <line x1="20" y1="32" x2="44" y2="32" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="20" y1="36" x2="44" y2="36" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="20" y1="40" x2="44" y2="40" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="20" y1="44" x2="44" y2="44" stroke="#bdc3c7" stroke-width="1"/>
  
  <line x1="24" y1="28" x2="24" y2="48" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="28" y1="28" x2="28" y2="48" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="32" y1="28" x2="32" y2="48" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="36" y1="28" x2="36" y2="48" stroke="#bdc3c7" stroke-width="1"/>
  <line x1="40" y1="28" x2="40" y2="48" stroke="#bdc3c7" stroke-width="1"/>
  
  <!-- Clock overlay -->
  <circle cx="38" cy="38" r="8" fill="#e74c3c" stroke="white" stroke-width="2"/>
  
  <!-- Clock hands -->
  <line x1="38" y1="38" x2="38" y2="32" stroke="white" stroke-width="2" stroke-linecap="round"/>
  <line x1="38" y1="38" x2="42" y2="38" stroke="white" stroke-width="1.5" stroke-linecap="round"/>
  
  <!-- Clock center dot -->
  <circle cx="38" cy="38" r="1.5" fill="white"/>
